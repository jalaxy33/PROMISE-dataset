log4j.rootCategory=DEBUG, A
log4j.configDebug=true
log4j.category.org.apache.log4j.test.ShortSocketServer=WARN
log4j.category.org.apache.log4j.net.SocketNode=WARN
log4j.appender.A=org.apache.log4j.FileAppender
log4j.appender.A.File=  temp 
log4j.appender.A.Append=false 
log4j.appender.A.layout=org.apache.log4j.PatternLayout
log4j.appender.A.layout.ConversionPattern=%5p %x [%t] %C (%F:%L) %m%n
